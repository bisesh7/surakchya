Sign In\n        </NavLink>\n      </NavItem>\n      <NavItem>\n        <NavLink\n          onClick={(e) => {\n            e.preventDefault();\n            props.history.push(\"/register\");\n          }}\n          href=\"/register\"\n        >\n          Register\n        </NavLink>\n      </NavItem>\n    </Fragment>\n  );\n\n  const authLinks = (\n    <Fragment>\n      <NavItem>\n        <NavLink onClick={signOutPressed} href=\"/signout\">\n          Sign Out\n        </NavLink>\n      </NavItem>\n    </Fragment>\n  );\n\n  return (\n    <div>\n      <Navbar color=\"light\" light expand=\"md\">\n        <NavbarBrand href=\"/\">Surakchya</NavbarBrand>\n        <NavbarToggler onClick={toggle} />\n        <Collapse isOpen={isOpen} navbar>\n          <Nav className=\"ml-auto\" navbar>\n            <NavItem>\n              <NavLink href=\"https://github.com/bisesh7/surakchya\">\n                GitHub\n              </NavLink>\n            </NavItem>\n            {userIsLoggedIn ? authLinks : guestLinks}\n          </Nav>\n        </Collapse>\n      </Navbar>\n    </div>\n  );\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    userSignOut: () => {\n      return axios\n        .get(\"/api/auth/signout\", {\n          headers: {\n            API_KEY: process.env.REACT_APP_API_KEY,\n          },\n        })\n        .then(() => {\n          dispatch(userSignOut());\n        });\n    },\n  };\n};\n\nexport default connect(null, mapDispatchToProps)(NavbarComponent);\n","/home/bisesh/Documents/hotel-reservation-system/front/src/Components/SigninComponent.js",[],"/home/bisesh/Documents/hotel-reservation-system/front/src/Components/RegisterComponent.js",[],"/home/bisesh/Documents/hotel-reservation-system/front/src/Functions/Functions.js",[],"/home/bisesh/Documents/hotel-reservation-system/front/src/Reducers/rootReducer.js",[],"/home/bisesh/Documents/hotel-reservation-system/front/src/Actions/authActions.js",[],"/home/bisesh/Documents/hotel-reservation-system/front/src/Actions/authActionTypes.js",[],"/home/bisesh/Documents/hotel-reservation-system/front/src/Components/Home/HomeComponent.js",["72"],"import React, { useEffect } from \"react\";\nimport NavbarComponent from \"../NavbarComponent\";\nimport { connect } from \"react-redux\";\nimport { userSignIn } from \"../../Actions/authActions\";\nimport axios from \"axios\";\nimport FrontPictureComponent from \"./FrontPictureComponent\";\nimport SearchBarComponent from \"./SearchBarComponent\";\nimport ExploreAndLiveAnywhereComponent from \"./ExploreAndLiveAnywhereComponent\";\nimport JoinAsAHostComponent from \"./JoinAsAHostComponent\";\n\nconst HomeComponent = (props) => {\n  useEffect(() => {\n    props.userSignIn();\n  }, []);\n\n  useEffect(() => {\n    console.log(props.user);\n  }, [props]);\n\n  return (\n    <div>\n      <NavbarComponent {...props} />\n      <SearchBarComponent />\n      <FrontPictureComponent />\n      <ExploreAndLiveAnywhereComponent />\n      <JoinAsAHostComponent />\n    </div>\n  );\n};\n\nconst mapStateToProps = (state) => {\n  return {\n    user: state.user,\n  };\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    userSignIn: () => {\n      // Check if the user is logged in.\n      // If logged in, we will get the user and update the redux store\n      // by passing a action else we do nothing\n      return axios\n        .get(\"/api/auth\", {\n          headers: {\n            API_KEY: process.env.REACT_APP_API_KEY,\n          },\n        })\n        .then((res) => {\n          dispatch(userSignIn(res.data.userDetail));\n        })\n        .catch((err) => {\n          if (err.response.status === 401) {\n            console.log(\"Not logged in\");\n          }\n        });\n    },\n  };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(HomeComponent);\n","/home/bisesh/Documents/hotel-reservation-system/front/src/Components/Home/FrontPictureComponent.js",["73"],"import React from \"react\";\nimport { Container